{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:react/jsx-runtime",
        "plugin:jsx-a11y/recommended",
        "standard",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "jsx-a11y"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "react/display-name": "warn"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
     }
}
